<div class="header">
  <h1>Welcome to the lottery game</h1>
</div>
<div class="gamble">
  <div class="lottery">
    <div class="lottery-header">
      Your lottery games
      </div>
    <div class="start-button">
      <button type="button" class="lotteryB btn btn-primary">Start</button>
    </div>
    <div class="results-lottery">
      <ul class="list">
        <li>
          <%=displayResultLot = rand(0..99999) %>
        </li>
      </ul>
    </div>
  </div>
  <div class="eurojackpot">
    <div class="lottery-header">
      Your eurjackpot games
    </div>
    <div class="start-button">
      <button type="button" class="eurojackB btn btn-primary">Start</button>
    </div>
      <div class="results-eurojack">
        <ul class="list">
          <li>
            <%= displayResultEuro = 5.times do rand(1..50) end %>
            <% end %>
            Soles
            <% 2.times do  %>
            <%= rand(1..12) %>
            <% end %>
          </li>
        </ul>
      </div>
  </div>
  <div class="primitive">
    <div class="lottery-header">
      Your primitive games
    </div>
    <div class="start-button">
      <button type="button" class="primitiveB btn btn-primary">Start</button>
    </div>
      <div class="results-primitive">
        <ul class="list">
          <li>
            <% 6.times do  %>
            <%= rand(0..49) %>
            <% end %>
            Reintegro
            <%= rand(0..10) %>
          </li>
        </ul>
      </div>
  </div>
</div>
<div class="final-result">
  <div class="final-result-lottery">
    <%= displayResultLot %>
  </div>
</div>




